{
  "$schema": "https://opencode.ai/config.json",
  "theme": "system",
  "model": "anthropic/claude-3-5-sonnet-latest",
  "permission": {
    "edit": "allow",
    "bash": {
      "*": "allow"
    }
  },
  "keybinds": {
    "input_submit": "enter",
    "input_newline": "shift+enter,ctrl+j",
    "session_new": "ctrl+n",
    "input_clear": "ctrl+k",
    "agent_cycle": "tab",
    "agent_cycle_reverse": "shift+tab"
  }
}